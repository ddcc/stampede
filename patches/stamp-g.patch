+        while (LIST_ITER_HASNEXT(&it, neighborListPtr)) {
+            element_t* neighborPtr = (element_t*)LIST_ITER_NEXT(&it, neighborListPtr);
+            Op op = { REMOVE_NEIGHBOR, neighborPtr, 0, element };
+            ops.push_back(op);
+        }
+        Op op = { SET_GARBAGE, element, TRUE, 0 };
+        ops.push_back(op);
+    }
+
