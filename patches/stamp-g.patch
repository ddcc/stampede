+    learner_task_t* aTaskPtr = (learner_task_t*)aPtr;
+    learner_task_t* bTaskPtr = (learner_task_t*)bPtr;
+    float aScore = aTaskPtr->score;
+    float bScore = bTaskPtr->score;
+
+    if (aScore < bScore) {
+        return 1;
+    } else if (aScore > bScore) {
+        return -1;
+    } else {
+        return (aTaskPtr->toId - bTaskPtr->toId);
+    }
+}
