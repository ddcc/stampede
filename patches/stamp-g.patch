-static void
-initEdges (element_t* elementPtr, coordinate_t* coordinates, long numCoordinate)
-{
-    long numEdge = ((numCoordinate * (numCoordinate - 1)) / 2);
-
-    elementPtr->numEdge = numEdge;
-
-    long e;
-    for (e = 0; e < numEdge; e++) {
-        setEdge(elementPtr, e);
-    }
-}
