+static void
+testCounts (adtree_t* adtreePtr, data_t* dataPtr)
+{
+    long numVar = dataPtr->numVar;
+    vector_t* queryVectorPtr = vector_alloc(numVar);
+    long v;
+    for (v = -1; v < numVar; v++) {
+        testCount(adtreePtr, dataPtr, queryVectorPtr, v, dataPtr->numVar);
+    }
+    vector_free(queryVectorPtr);
+}
