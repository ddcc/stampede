+#ifdef LEARNER_TRY_REVERSE
+            case OPERATION_REVERSE: {
+                TM_BEGIN();
+                TMpopulateQueryVectors(TM_ARG
+                                       netPtr,
+                                       fromId,
+                                       queries,
+                                       &queryVector,
+                                       &parentQueryVector);
+                newBaseLogLikelihood =
