+ */
+void
+net_free (net_t* netPtr)
+{
+    long i;
+    NodeVector* nodeVectorPtr = netPtr->nodeVectorPtr;
+    long numNode = nodeVectorPtr->size();
+    for (i = 0; i < numNode; i++) {
+        net_node_t* nodePtr = nodeVectorPtr->at(i);
+        freeNode(nodePtr);
+    }
+    delete netPtr->nodeVectorPtr;
+    delete netPtr;
