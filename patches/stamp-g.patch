-router_solve (void* argPtr)
-{
-    TM_THREAD_ENTER();
-
-    router_solve_arg_t* routerArgPtr = (router_solve_arg_t*)argPtr;
-    router_t* routerPtr = routerArgPtr->routerPtr;
-    maze_t* mazePtr = routerArgPtr->mazePtr;
