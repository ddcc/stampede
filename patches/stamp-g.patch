+static bool_t
+list_insert_helper(list_t* p, void* k) {
+    p->push_front(k);
+    auto ii = p->begin();
+    auto ei = p->end();
+    auto jj = ii;
+    ++jj;
+
+    for (; jj != ei; ++ii, ++jj) {
+        if (*ii > *jj)
+            std::swap(*ii, *jj);
+        else
+            break;
+    }
+    return TRUE;
+}
