-/* =============================================================================
- * compareSegment
- * -- For hashtable
- * =============================================================================
- */
-static long
-compareSegment (const pair_t* a, const pair_t* b)
-{
-    return strcmp((char*)(a->firstPtr), (char*)(b->firstPtr));
-}
