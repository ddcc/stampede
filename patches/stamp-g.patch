+    varyPtr = (adtree_vary_t*)malloc(sizeof(adtree_vary_t));
+    if (varyPtr) {
+        varyPtr->index = index;
+        varyPtr->mostCommonValue = -1;
+        varyPtr->zeroNodePtr = NULL;
+        varyPtr->oneNodePtr = NULL;
+    }
+
+    return varyPtr;
