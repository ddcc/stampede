+// Helper to maintain sorted id order
+static bool_t list_remove_helper(list_t* p, void* k) {
+    auto ii = std::find(p->begin(), p->end(), k);
+    auto ei = p->end();
+    if (ii == ei)
+        return FALSE;
+    auto jj = ii;
+    ++jj;
+
+    for (; jj != ei; ++ii, ++jj) {
+        std::swap(*ii, *jj);
+    }
+    p->pop_back();
+    return TRUE;
+}
