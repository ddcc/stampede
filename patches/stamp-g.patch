+{
+    element_t* aElementPtr = (element_t*)aPtr;
+    element_t* bElementPtr = (element_t*)bPtr;
+
+    if (aElementPtr->encroachedEdgePtr) {
+        if (bElementPtr->encroachedEdgePtr) {
+            return 0; /* do not care */
+        } else {
+            return 1;
+        }
+    }
+
+    if (bElementPtr->encroachedEdgePtr) {
+        return -1;
+    }
+
+    return 0; /* do not care */
+}
