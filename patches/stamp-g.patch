+    adtreePtr = (adtree_t*)malloc(sizeof(adtree_t));
+    if (adtreePtr) {
+        adtreePtr->numVar = -1L;
+        adtreePtr->numRecord = -1L;
+        adtreePtr->rootNodePtr = NULL;
