+static bool_t list_remove_helper(list_t* p, void* k, long (*compare)(const void*, const void*)) {
+    auto ii = p->begin();
+    auto ei = p->end();
+    while (ii != ei) {
+        if (compare(*ii, k) == 0)
+            break;
+        ++ii;
+    }
+    if (ii == ei)
+        return FALSE;
+
+    std::swap(*ii, p->back());
+    p->pop_back();
+    return TRUE;
+}
